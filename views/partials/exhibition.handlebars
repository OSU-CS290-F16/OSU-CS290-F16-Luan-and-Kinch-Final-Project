<section class="exhibition-section">
    <a href="{{url}}"><img src="{{url}}" alt="" width="426.66px" height="240px"></a>
    <div class="">
        <p>ID: {{_id}}</p>
        <p>NAME: {{name}}</p>
        <p>OWNER: {{owner}}</p>
        <p>SIZE: {{width}} * {{height}}</p>
        <p>DESCRIPTION: {{description}}</p>
    </div>
    
    <div class="dismiss-button" onclick="deleteItem('{{_id}}')">X</div>
</section>
